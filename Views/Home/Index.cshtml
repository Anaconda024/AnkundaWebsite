<section class="site-section bg-light bg-image" id="contact-section">
    <div class="container">
        <div class="row mb-5">
            <div class="col-12 text-center">
                <!-- <h3 class="section-sub-title">Get</h3> -->
                <h2 class="section-title mb-3">Contact Me</h2>
            </div>
        </div>
        <div class="row">
            <div class="col-md-7 mb-5">
                <form asp-action="Create" class="p-5 bg-white">

                    <h2 class="h4 text-black mb-5">Contact Form</h2>

                    <div class="row form-group">
                        <div class="col-md-6 mb-3 mb-md-0">
                            <label asp-for="FirstName" class="text-black" for="fname">First Name</label>
                            <input asp-for="FirstName" type="text" id="fname" class="form-control">
                        </div>
                        <div class="col-md-6">
                            <label asp-for="LastName" class="text-black" for="lname">Last Name</label>
                            <input asp-for="LastName" type="text" id="lname" class="form-control">
                        </div>
                    </div>

                    <div class="row form-group">

                        <div class="col-md-12">
                            <label asp-for="Email" class="text-black" for="email">Email</label>
                            <input asp-for="Email" type="email" id="email" class="form-control">
                        </div>
                    </div>

                    <div class="row form-group">

                        <div class="col-md-12">
                            <label asp-for="CellNumber" class="text-black" for="subject">Cellphone Number</label>
                            <input asp-for="CellNumber" type="subject" id="subject" class="form-control">
                        </div>
                    </div>

                    <div class="row form-group">
                        <div class="col-md-12">
                            <label asp-for="Message" class="text-black" for="message">Message</label>
                            <textarea asp-for="Message" name="message" id="message" cols="30" rows="7" class="form-control" placeholder="Write your notes or questions here..."></textarea>
                        </div>
                    </div>

                    <div class="row form-group">
                        <div class="col-md-12">
                            <input type="submit" value="Submit" class="btn btn-primary btn-md text-white">
                        </div>
                    </div>

                </form>
            </div>
            <div class="col-md-5">

                <div class="p-4 mb-3 bg-white">
                    <p class="mb-0 font-weight-bold">Address</p>
                    <p class="mb-4">1 Suikerbossie Street, Ululapa Complex, Johannesburg</p>

                    <p class="mb-0 font-weight-bold">Phone</p>
                    <p class="mb-4"><a href="#">083 398 4688</a></p>

                    <p class="mb-0 font-weight-bold">Email Address</p>
                    <p class="mb-0"><a href="#">ankundamb@gmail.com</a></p>

                </div>
            </div>
        </div>
    </div>
</section>