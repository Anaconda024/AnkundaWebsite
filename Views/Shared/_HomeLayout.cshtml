            <div class="row">
                <div class="col-md-12">
                    <div class="row">
                        <div class="col-md-3">
                            <partial name="_LoginPartial" />
                            @if (SignInManager.IsSignedIn(User))
                            {                                
                                <li class="nav-item">
                                    <a class="nav-link text-dark " asp-controller="Home" asp-action="ViewInteracts"> View Interacts</a>
                                </li>
